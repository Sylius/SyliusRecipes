{
    "manifests": {
        "sylius/plus-marketplace-suite-plugin": {
            "manifest": {
                "bundles": {
                    "Sylius\\MarketplaceSuite\\SyliusMarketplaceSuitePlugin": [
                        "all"
                    ]
                },
                "copy-from-recipe": {
                    "config/": "%CONFIG_DIR%/",
                    "src/": "%SRC_DIR%/"
                },
                "add-lines": [
                    {
                        "file": "config/packages/security.yaml",
                        "content": "        - { path: \"%sylius.security.shop_regex%/account/vendor/conversations/create\", role: ROLE_USER }\n        - { path: \"%sylius.security.shop_regex%/account/vendor/conversations\", role: ROLE_USER }\n        - { path: \"%sylius.security.shop_regex%/account/vendor/register\", role: ROLE_USER }\n        - { path: \"%sylius.security.shop_regex%/account/vendor\", role: ROLE_VENDOR }\n        - { path: \"%sylius.security.shop_regex%/account\", role: ROLE_USER }\n\n        - { path: \"%sylius.security.api_shop_account_vendor_regex%/register\", role: ROLE_USER }\n        - { path: \"%sylius.security.api_shop_account_vendor_regex%\", role: ROLE_VENDOR }",
                        "position": "after_target",
                        "target": "    access_control:",
                        "warn_if_missing": true
                    },
                    {
                        "file": "assets/admin/entrypoint.js",
                        "content": "import '@vendor/sylius/plus-marketplace-suite-plugin/assets/admin/entrypoint';",
                        "position": "bottom",
                        "warn_if_missing": true
                    },
                    {
                        "file": "assets/shop/entrypoint.js",
                        "content": "import '@vendor/sylius/plus-marketplace-suite-plugin/assets/shop/entrypoint';",
                        "position": "bottom",
                        "warn_if_missing": true
                    }
                ],
                "env": {
                    "VENDOR_PRODUCTS_LIMITS": "9,18,27",
                    "DEFAULT_VENDOR_PRODUCTS_LIMIT": "9",
                    "LOGO_DIRECTORY": "media/image/logo/",
                    "ADMIN_ATTACHMENTS_UPLOAD_DIRECTORY": "uploads/attachments",
                    "MESSAGES_FILE_UPLOAD_DIRECTORY": "uploads/message_files",
                    "VENDOR_B2B_REGISTRATION": "0",
                    "DEFAULT_VENDOR_LISTING_AUTO_VERIFY": "0",
                    "VENDOR_PROFILE_AUTO_ACCEPTANCE": "0",
                    "DEFAULT_VENDOR_COMMISSION": "10",
                    "DEFAULT_VENDOR_COMMISSION_TYPE": "gross"
                }
            },
            "files": {
                "config/packages/sylius_plus_marketplace_suite_plugin.yaml": {
                    "contents": [
                        "imports:",
                        "    - { resource: \"@SyliusMarketplaceSuitePlugin/config/config.yaml\" }",
                        "",
                        "sylius_channel:",
                        "    resources:",
                        "        channel:",
                        "            classes:",
                        "                repository: App\\Repository\\ChannelRepository",
                        "",
                        "sylius_customer:",
                        "    resources:",
                        "        customer:",
                        "            classes:",
                        "                repository: App\\Repository\\CustomerRepository",
                        "",
                        "sylius_order:",
                        "    resources:",
                        "        order:",
                        "            classes:",
                        "                repository: App\\Repository\\OrderRepository",
                        "                controller: Sylius\\MarketplaceSuite\\Component\\Core\\Common\\Controller\\Resource\\OrderController",
                        "",
                        "sylius_payment:",
                        "    resources:",
                        "        payment:",
                        "            classes:",
                        "                repository: App\\Repository\\PaymentRepository",
                        "",
                        "sylius_product:",
                        "    resources:",
                        "        product:",
                        "            classes:",
                        "                repository: App\\Repository\\ProductRepository",
                        "        product_variant:",
                        "            classes:",
                        "                repository: App\\Repository\\ProductVariantRepository",
                        "",
                        "sylius_review:",
                        "    resources:",
                        "        product:",
                        "            review:",
                        "                classes:",
                        "                    repository: App\\Repository\\ProductReviewRepository",
                        "",
                        "sylius_shipping:",
                        "    resources:",
                        "        shipment:",
                        "            classes:",
                        "                repository: App\\Repository\\ShipmentRepository",
                        "",
                        "sylius_taxonomy:",
                        "    resources:",
                        "        taxon:",
                        "            classes:",
                        "                repository: App\\Repository\\TaxonRepository",
                        ""
                    ],
                    "executable": false
                },
                "config/routes/sylius_plus_marketplace_suite_plugin.yaml": {
                    "contents": [
                        "sylius_marketplace_suite:",
                        "    resource: \"@SyliusMarketplaceSuitePlugin/config/routes.yaml\"",
                        ""
                    ],
                    "executable": false
                },
